generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Todo {
  id        String   @id @map("_id") @default(auto()) @db.ObjectId
  title     String
  description     String
  status      Boolean  @default(false)
  createdAt DateTime @default(now())
}
